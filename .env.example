# Replicate API Token (get from https://replicate.com/account/api-tokens)
REPLICATE_API_TOKEN=your_token_here

# Vercel Blob Storage (automatically added when you create a blob store)
# Create with: vercel blob create comic-files
BLOB_READ_WRITE_TOKEN=vercel_blob_rw_your_token_here

# Optional: Node Environment
NODE_ENV=development

# Optional: Server Port (for local development)
PORT=3000
