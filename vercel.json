{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 300,
      "memory": 3008
    }
  }
}
